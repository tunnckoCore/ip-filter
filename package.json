{
  "name": "ip-filter",
  "version": "2.0.0",
  "description": "Validates valid IPs (IPv4 and IPv6) using [micromatch][] - glob patterns, RegExp, string or array of globs. If match returns the IP, otherwise null.",
  "repository": "tunnckoCore/ip-filter",
  "author": "Charlike Mike Reagent <@tunnckoCore> (http://www.tunnckocore.tk)",
  "precommit.silent": true,
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "lint": "standard --verbose",
    "pretest": "npm run lint",
    "test": "npm run coverage",
    "posttest": "npm run lint:coverage",
    "coverage": "nyc node test.js",
    "lint:coverage": "nyc check-coverage --lines 100 --branches 100 --statements 100",
    "report-coverage": "nyc report --reporter=text-lcov | coveralls",
    "prerelease": "npm test",
    "release": "standard-version --sign --no-verify",
    "precommit": "git add --all",
    "commit": "git-cz"
  },
  "dependencies": {
    "ip-regex": "^1.0.3",
    "is-match": "^0.4.0",
    "to-file-path": "^1.0.0"
  },
  "devDependencies": {
    "commitizen": "^2.8.6",
    "coveralls": "^2.11.12",
    "cz-conventional-changelog": "^1.2.0",
    "mukla": "^0.4.1",
    "nyc": "^8.1.0",
    "pre-commit": "^1.1.3",
    "standard": "^8.0.0",
    "standard-version": "^2.4.0"
  },
  "files": [
    "index.js",
    "utils.js"
  ],
  "keywords": [
    "array",
    "check",
    "filter",
    "glob",
    "ip",
    "ip-filter",
    "ips",
    "ipv",
    "ismatch",
    "match",
    "matcher",
    "micro",
    "micromatch",
    "modern",
    "patterns",
    "regexp",
    "string",
    "valid",
    "validate"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "verb": {
    "run": true,
    "toc": false,
    "layout": "empty",
    "tasks": [
      "readme"
    ],
    "related": {
      "list": [
        "to-file-path",
        "ip-regex",
        "is-match",
        "micromatch",
        "koa-ip-filter",
        "is-match-ip"
      ]
    },
    "reflinks": [
      "is-match",
      "koa-ip-filter",
      "to-file-path",
      "to-object-path",
      "ip-filter",
      "micromatch"
    ],
    "lint": {
      "reflinks": true
    }
  }
}
